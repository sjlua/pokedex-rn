# Expo Configuration
# Copy this file to .env.local and fill in your values

# Expo Access Token (for CI/CD)
# Generate at: https://expo.dev/accounts/[account]/settings/access-tokens
EXPO_TOKEN=your-expo-token-here

# Apple Configuration (for iOS builds)
APPLE_ID=your-apple-id@example.com
ASC_APP_ID=your-app-store-connect-app-id
APPLE_TEAM_ID=your-apple-team-id

# Android Configuration (for Play Store submissions)
# Path to Google Play service account JSON key file
GOOGLE_SERVICE_ACCOUNT_KEY_PATH=./path/to/service-account-key.json
